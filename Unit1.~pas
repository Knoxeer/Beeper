procedure TBeeper1.Execute;
begin
  while not Terminated do
  begin
      Windows.Beep(330, 100); Sleep(100);if uStopBeeping then Exit;
			Windows.Beep(330, 100); Sleep(300);if uStopBeeping then Exit;
			Windows.Beep(330, 100); Sleep(300);if uStopBeeping then Exit;
