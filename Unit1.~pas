procedure TForm1.Button71Click(Sender: TObject);
begin
    // установить будильник
    AlHour := UpDown1.Position;
    AlMin  := UpDown2.Position;
    Timer45.Tag := 1;
    Timer45.Interval := 3000; // проверять каждые 3 секунды
end;

