procedure TForm1.Button70Click(Sender: TObject);
begin
Timer44.Enabled := True;
end;

procedure TForm1.Timer44Timer(Sender: TObject);
var
H,M,S:Byte;
begin
Dec(EndTime);
S:= EndTime mod 60;
M:= EndTime div 60 mod 60;
H:= EndTime div 3600;
Label26.Caption:= IntToStr(H) + ':' + IntToStr(M) + ':' + IntToStr(S);
if EndTime =0 then
begin
 StartBeeping22;
 end;
 end;

