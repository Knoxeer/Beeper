			Windows.Beep(587, 110);if uStopBeeping then Exit;
			Windows.Beep(494, 230);if uStopBeeping then Exit;
			Windows.Beep(740, 230);if uStopBeeping then Exit;
			Windows.Beep(659, 460);if uStopBeeping then Exit;
