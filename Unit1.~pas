//
procedure TSyncThread1.Execute;
begin
  while not Terminated do
  begin
      Windows.Beep(330, 100); Sleep(100);if uStopBeeping then Exit; Progress:=1;Synchronize(SetProgress);
			Windows.Beep(330, 100); Sleep(300);if uStopBeeping then Exit; Progress:=2;Synchronize(SetProgress);
			Windows.Beep(330, 100); Sleep(300);if uStopBeeping then Exit; Progress:=3;Synchronize(SetProgress);
			Windows.Beep(262, 100); Sleep(100);if uStopBeeping then Exit; Progress:=4;Synchronize(SetProgress);
			Windows.Beep(330, 100); Sleep(300);if uStopBeeping then Exit; Progress:=5;Synchronize(SetProgress);
			Windows.Beep(392, 100); Sleep(700);if uStopBeeping then Exit; Progress:=6;Synchronize(SetProgress);
			Windows.Beep(196, 100); Sleep(700);if uStopBeeping then Exit; Progress:=7;Synchronize(SetProgress);

			// Parte 1
			Windows.Beep(262, 300); Sleep(300);if uStopBeeping then Exit; Progress:=8;Synchronize(SetProgress);
			Windows.Beep(196, 300); Sleep(300);if uStopBeeping then Exit; Progress:=9;Synchronize(SetProgress);
			Windows.Beep(164, 300); Sleep(300);if uStopBeeping then Exit; Progress:=10;Synchronize(SetProgress);
			Windows.Beep(220, 300); Sleep(100);if uStopBeeping then Exit; Progress:=11;Synchronize(SetProgress);
			Windows.Beep(246, 100); Sleep(300);if uStopBeeping then Exit; Progress:=12;Synchronize(SetProgress);

			Windows.Beep(233, 200);if uStopBeeping then Exit; Progress:=13;Synchronize(SetProgress);
			Windows.Beep(220, 100); Sleep(300);if uStopBeeping then Exit; Progress:=14;Synchronize(SetProgress);
			Windows.Beep(196, 100); Sleep(150);if uStopBeeping then Exit; Progress:=15;Synchronize(SetProgress);
			Windows.Beep(330, 100); Sleep(150);if uStopBeeping then Exit; Progress:=16;Synchronize(SetProgress);
			Windows.Beep(392, 100); Sleep(150);if uStopBeeping then Exit; Progress:=17;Synchronize(SetProgress);
			Windows.Beep(440, 100); Sleep(300);if uStopBeeping then Exit; Progress:=18;Synchronize(SetProgress);
			Windows.Beep(349, 100); Sleep(100);if uStopBeeping then Exit; Progress:=19;Synchronize(SetProgress);
			Windows.Beep(392, 100); Sleep(300);if uStopBeeping then Exit; Progress:=20;Synchronize(SetProgress);
			Windows.Beep(330, 100); Sleep(300);if uStopBeeping then Exit; Progress:=21;Synchronize(SetProgress);
			Windows.Beep(262, 100); Sleep(100);if uStopBeeping then Exit; Progress:=22;Synchronize(SetProgress);
			Windows.Beep(294, 100); Sleep(100);if uStopBeeping then Exit; Progress:=23;Synchronize(SetProgress);
			Windows.Beep(247, 100); Sleep(500);if uStopBeeping then Exit; Progress:=24;Synchronize(SetProgress);


			Windows.Beep(262, 300); Sleep(300);if uStopBeeping then Exit; Progress:=25;Synchronize(SetProgress);
			Windows.Beep(196, 300); Sleep(300);if uStopBeeping then Exit; Progress:=26;Synchronize(SetProgress);
			Windows.Beep(164, 300); Sleep(300);if uStopBeeping then Exit; Progress:=27;Synchronize(SetProgress);
			Windows.Beep(220, 300); Sleep(100);if uStopBeeping then Exit; Progress:=28;Synchronize(SetProgress);
			Windows.Beep(246, 100); Sleep(300);if uStopBeeping then Exit; Progress:=29;Synchronize(SetProgress);

			Windows.Beep(233, 200);if uStopBeeping then Exit;
